      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{auth_token}}",
            "type": "text"
          }
        ],
        "url": {
          "raw": "{{base_url}}/api/v1/chit-chat-shipments/{{shipment_id}}",
          "host": ["{{base_url}}"],
          "path": ["api", "v1", "chit-chat-shipments", "{{shipment_id}}"]
        },
        "description": "Get details of a specific shipment"
      },
      "response": []
    },
    {
      "name": "Update Shipment",
      "request": {
        "method": "PATCH",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{auth_token}}",
            "type": "text"
          },
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"description\": \"Updated description\",\n  \"value\": \"150.00\"\n}"
        },
        "url": {
          "raw": "{{base_url}}/api/v1/chit-chat-shipments/{{shipment_id}}",
          "host": ["{{base_url}}"],
          "path": ["api", "v1", "chit-chat-shipments", "{{shipment_id}}"]
